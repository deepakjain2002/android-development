<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.deepakjain.androiddevelopmentloginpage">

    <application
        android:allowBackup="true"
        android:icon="@drawable/login"
        android:label="@string/app_name"
        android:roundIcon="@drawable/login"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidDevelopmentLoginPage">
        <activity android:name=".RecyclerViewList"></activity>
        <activity android:name=".HomeScreen" />
        <activity android:name=".RegisterUser" />
        <activity android:name=".HomePage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>